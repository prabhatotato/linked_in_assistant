{
    "manifest_version": 3,
    "name": "Saved in",
    "version": "1.0",
    "description": " A linked in extesion working as a voice assistant fo linked in",

    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      },

    "content_scripts": [
    {
      "js": [ "scripts/content.js"],
      "css": ["scripts/content.css"],
      "matches": [
        "https://www.linkedin.com/*"
        
      ]
    }
  ]
}